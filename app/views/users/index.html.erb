<% provide(:title, 'All users') %>
<h1>All users</h1>
<div class="row">
    <div class="col-md-4 col-md-offset-3 ">
    <%= form_tag search_path, :method => 'get' do %>
    <%= text_field_tag :search, params[:search], required: true, class:"form form-control" %>
    <%= submit_tag "Search", class:"form form-control btn btn-primary"%>
    <% end %>
    </div>
</div>
<%= will_paginate %>
    <ul class="users">
        <%= render @users %>
    </ul>
<%= will_paginate %>