<% provide(:title, 'All users') %>
<h1>All users</h1>
<div>
    <%= form_tag search_path, :method => 'get' do %>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search"%>
    <% end %>
</div>
<%= will_paginate %>
    <ul class="users">
        <%= render @users %>
    </ul>
<%= will_paginate %>