
<%= form_tag search_path, :method => 'get' do %>
<div class="col-xs-12" id="search_bar_form_container">
    <div class=""><%= text_field_tag :search, params[:search], class:"form-control", id:"input" %></div>
    <div class=""><%= submit_tag "Search", class:"btn btn-default", id:"button" %></div>
    </div>
<% end %>